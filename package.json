{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:server": "vite build --ssr src/entry-server.ts --outDir build/server-bundle",
    "build:client": "rimraf build && vue-tsc --noEmit && vite build --ssrManifest",
    "prebuild": "yarn build:client && yarn build:server",
    "build": "ts-node prerender.ts",
    "postbuild": "rimraf build/ssr-manifest.json build/server-bundle",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^17.0.10",
    "@types/prettier": "^2.4.3",
    "@vitejs/plugin-vue": "^2.0.0",
    "html-minifier": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
